{
  "name": "in2code/femanagerextended",
  "description": "Example addon extension to TYPO3 extension femanager",
  "keywords": [
    "typo3",
    "form",
    "femanager",
    "fe_users",
    "registration"
  ],
  "homepage": "https://github.com/in2code-de/femanagerextended",
  "authors": [
    {
      "name": "Alex Kellner",
      "email": "alexander.kellner@in2code.de"
    },
    {
      "name": "Daniel Hoffmann",
      "email": "daniel.hoffmann@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-3.0",
  "require": {
    "in2code/femanager": "^8.0 || dev-main || dev-develop || dev-eap || dev-eap-v12"
  },
  "autoload": {
    "psr-4": {
      "In2code\\Femanagerextended\\": "Classes"
    }
  },
  "scripts": {
    "test:php:lint": "find *.php Classes Configuration Tests -name '*.php' -print0 | xargs -0 -n 1 -P 4 php -l"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "femanagerextended"
    }
  }
}
